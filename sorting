def sort(nums):
    for i in range(9):
        minpos = i
        for j in range(i,10):
             if nums[j] < nums[minpos]:
               minpos = j

             temp = nums[i]
             nums[i] =nums[minpos]
             nums[minpos] = temp

             #print(nums)

nums = [8,6,3,2,1,0,5,7,9,4]
sort(nums)
print(nums)
print(sort(nums))
print(sorted(nums))
